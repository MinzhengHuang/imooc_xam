<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xam"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/xam"
        android:label="@string/app_name"
        android:theme="@style/MMTheme.Basic" >
        <activity
            android:name="com.xam.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.xam.LibraryActivity" >
        </activity>
        <activity android:name="com.xam.ClearEditTextActivity" >
        </activity>
        <activity android:name="com.xam.CircleProgressActivity" >
        </activity>
        <activity android:name="com.xam.DragGridViewActivity" >
        </activity>
        <activity android:name="com.xam.PhotoAlbumActivity" >
        </activity>
        <activity android:name="com.xam.ShowPhotoActivity" >
        </activity>
        <activity android:name="com.xam.SwipeActivity" >
        </activity>
        <activity android:name="com.xam.SortListViewActivity" >
        </activity>
        <activity android:name="com.xam.SlideCutListViewActivity" >
        </activity>
        <activity android:name="com.xam.ImageScanActivity" >
        </activity>
        <activity android:name="com.xam.ShowImageActivity" >
        </activity>
    </application>

</manifest>